{
  "name": "react-stomp",
  "version": "4.1.0",
  "description": "React websocket component with STOMP over SockJS",
  "main": "./dist/client.js",
  "nyc": {
    "extension": [
      ".jsx"
    ]
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/react-redux.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/react-redux.min.js",
    "clean": "rimraf lib dist es coverage",
    "coverage": "codecov",
    "lint": "eslint src test/utils test/components",
    "prepare": "npm run clean && npm run build",
    "test": "node ./test/run-tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lahsivjar/react-stomp.git"
  },
  "keywords": [
    "react",
    "websocket",
    "sockjs",
    "stomp"
  ],
  "author": "lahsivjar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lahsivjar/react-stomp/issues"
  },
  "homepage": "https://github.com/lahsivjar/react-stomp#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "documentation": "^9.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdom": "^13.0.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sinon": "^6.2.0"
  },
  "dependencies": {
    "net": "^1.0.2",
    "sockjs-client": "^1.1.5",
    "stompjs": "^2.3.3"
  }
}
